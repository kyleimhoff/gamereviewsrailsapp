<h1>Sign up! </h1>

<%= form_for @user, url: signup_path do |t|%>
    <%= t.label :username%>
    <%= t.text_field :username%>
    <br>
    <%= t.label :password%>
    <%= t.password_field :password%>
    <br>
    <%= t.submit "Submit"%>
<% end %>
<%= link_to('Log in with Facebook!', '/auth/facebook') %>